(jbuild_version 1)

(library
 ((name        docker)
  (public_name docker-api)
  (synopsis "Binding to the Docker Remote API")
  (libraries (bytes unix yojson))))

(rule
 ((targets (docker_config.ml docker_utils.ml))
  (deps    (../compatibility.ml docker_config.pre.ml docker_utils.pre.ml))
  (action  (run ocaml ${<}))))
